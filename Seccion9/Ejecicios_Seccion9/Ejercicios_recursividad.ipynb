    "#Funciones recursivas\n",
    "#Ejercicio de codificacion 20: Funciones recursivas\n",
    "def sumatorios(numero):\n",
    "    if numero>0:\n",
    "        print(numero)\n",
    "        #recursivas\n",
    "        numero=numero+sumatorios(numero-1)\n",
    "        print(numero)\n",
    "    #print(numero)\n",
    "    return numero\n",
    "\n",
    "sumatorios(5)"
